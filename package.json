{
	"name": "tinder-backend",
	"version": "1.0.0",
	"main": "server.js",
	"type": "module",
	"engines": {
		"node": "14.15.4",
		"npm": "6.14.10"
	},
	"scripts": {
		"client": "cd tinder-clone && yarn start",
		"server": "nodemon server.js",
		"build": "cd tinder-clone && npm run build",
		"dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
		"start": "node server,js"
	},
	"author": "imarinho-dev <imarinho.dev@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"mongoose": "^5.11.17"
	},
	"devDependencies": {
		"concurrently": "^6.0.0"
	}
}
